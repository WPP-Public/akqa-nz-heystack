{
    "name": "heystack/heystack",
    "type": "silverstripe-module",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages:satis@packages.heyday.net.nz/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "*",
        "symfony/event-dispatcher": "2.*",
        "monolog/monolog": ">=1.0,<1.2-dev",
				"camspiers/shared-dependency-injection":"~0.1"
    },
    "require-dev": {
        "silverstripe/framework": "2.4.*",
        "composer/composer": "1.0.0-alpha6",
        "phpmd/phpmd": "1.4.*",
        "phploc/phploc": "dev-master",
        "pdepend/pdepend": "1.1.*",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Heystack": "code/"
        },
        "classmap": ["code/SilverStripe"]
    },
		"extra": {
				"branch-alias": {
						"dev-master": "1.1.x-dev"
        }
    },
    "minimum-stability": "dev"
}