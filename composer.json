{
    "name": "heystack/heystack",
    "type": "silverstripe-module",
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0",
        "heyday/symfony-autoinject": "~0.2.1",
        "symfony/event-dispatcher": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "symfony/console": "~2.4",
        "symfony/config": "~2.4",
        "psr/log": "~1.0",
        "doctrine/cache": "~1.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0",
        "mikey179/vfsStream": "~1.2.0",
        "silverstripe/framework": "~3.1@dev",
        "symfony/class-loader": "~2.4",
        "sami/sami": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Heystack\\Core\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}