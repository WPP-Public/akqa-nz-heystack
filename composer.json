{
    "name": "heystack/heystack",
    "type": "silverstripe-module",
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0.0",
        "symfony/event-dispatcher": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/console": "~2.3",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "silverstripe/framework": "~3.1@dev",
        "symfony/class-loader": "~2.4",
        "sami/sami": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Heystack\\Core\\": "src/"
        },
        "files": ["src/Util/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}